{
  "name": "my-qwik-basic-starter",
  "description": "Recommended for your first Qwik app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "npm run build.server.netlify",
    "build.server.netlify": "vite build -c adapters/netlify-edge/vite.config.ts",
    "build.server.fleek": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.fleek": "npm run build.types && npm run build.client && npm run build.server.fleek && npm run lint",
    "deploy": "netlify deploy --build",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "netlify dev",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.6.0",
    "@builder.io/qwik-city": "1.6.0",
    "@netlify/edge-functions": "^2.9.0",
    "@netlify/vite-plugin-netlify-edge": "1.1.3",
    "@types/eslint": "8.56.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "autoprefixer": "^10.4.19",
    "eslint": "9.6.0",
    "eslint-plugin-qwik": "1.6.0",
    "netlify-cli": "^17.33.0",
    "node-fetch": "3.3.2",
    "postcss": "^8.4.39",
    "prettier": "3.3.2",
    "sass": "^1.77.6",
    "tailwindcss": "^3.4.4",
    "typescript": "5.5.3",
    "undici": "^6.19.2",
    "vite": "5.3.3",
    "vite-tsconfig-paths": "4.3.2"
  },
  "dependencies": {
    "@qwikest/icons": "^0.0.13"
  },
  "packageManager": "pnpm@9.1.0+sha256.22e36fba7f4880ecf749a5ca128b8435da085ecd49575e7fb9e64d6bf4fad394"
}
