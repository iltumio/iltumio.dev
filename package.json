{
  "name": "iltumio.dev",
  "description": "My personal website",
  "engines": {
    "node": ">=22.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "bun run build.server.netlify",
    "build.server.netlify": "vite build -c adapters/netlify-edge/vite.config.mts",
    "build.server.static": "vite build -c adapters/static/vite.config.mts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "netlify deploy --build",
    "dev": "bun x vite --mode ssr",
    "dev.debug": "bun x node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint.fix": "eslint --fix \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "netlify dev",
    "start": "vite --open",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@eslint/js": "^9.39.2",
    "@netlify/edge-functions": "^3.0.3",
    "@netlify/vite-plugin-netlify-edge": "1.1.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/eslint": "9.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "autoprefixer": "^10.4.23",
    "eslint": "9.39.2",
    "eslint-plugin-qwik": "1.18.0",
    "netlify-cli": "^23.13.0",
    "node-fetch": "3.3.2",
    "postcss": "^8.5.6",
    "prettier": "3.7.4",
    "sass": "^1.97.1",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^5.0.0"
  },
  "dependencies": {
    "@qwikest/icons": "^0.0.13",
    "marked": "^17.0.1"
  },
  "packageManager": "bun@latest"
}
